# Python
cd python
docker build -f Dockerfile -t python-app ..
docker run -it python-app python main.py
docker run -it -v $(pwd)/..:/app -e DATA_DIR=data_100 python-app python python/main.py

# Ruby
cd ruby
docker build -f Dockerfile -t ruby-app ..
docker run -it ruby-app ruby main.rb
docker run -it -v $(pwd)/..:/app -e DATA_DIR=data_100 ruby-app ruby ruby/main.rb
